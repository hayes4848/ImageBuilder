<p id="notice"><%= notice %></p>

<h1>Image Handlers</h1>

<%= render 'form', image_handler: @image_handler %>


<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @image_handlers.each do |image_handler| %>
      <tr>
        <td><%= link_to 'Show', image_handler %></td>
        <td><%= link_to 'Edit', edit_image_handler_path(image_handler) %></td>
        <td><%= link_to 'Destroy', image_handler, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image Handler', new_image_handler_path %>
